<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             FlowDirection="RightToLeft"
             x:Class="SikumkumApp.Views.FilePage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding Headline}" Style="{x:StaticResource Headers}" HorizontalOptions="Center"/>
            <Label Text="{Binding Username}" Style="{x:StaticResource Headers}"/>


                    
                    
        </StackLayout>
    </ContentPage.Content>
</ContentPage>